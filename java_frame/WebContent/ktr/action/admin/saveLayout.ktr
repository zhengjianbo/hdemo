    <sql>&lt;%
	var webtemplate=&quot;&#47;&quot;+ID; 

	var webtemplate_info=&quot;&#47;&quot;+ID+&quot;_INFO&quot;;  &#47;&#47;配置信息

 
var result=false;
var message=&quot;&quot;;

 
		&#47;&#47;判断文件是否存在 
		if(isEmpty(ID)){
			result=true;
			message=&quot;内容为空，不保存 !&quot;; 
		}else{
			&#47;&#47;文件保存 	
try{
	 
		FileUtils.saveFileTemplateContent(webtemplate,tempateWebhtml);

		var  temp_info=&quot;{file:\&quot;&quot;+file+&quot;\&quot;,flag:\&quot;&quot;+flag+&quot;\&quot;,info:\&quot;&quot;+info+&quot;\&quot;,}&quot;;

		
		FileUtils.saveFileTemplateContent(webtemplate_info,temp_info);


         message=&quot;编辑成功 !&quot; ;
		  result=true;
}catch(error){
 
       message=&quot;保存内容发生错误:&quot; ;
	   
}
		}
		 	


%&gt;</sql>
