    <sql>&lt;%
	&#47;&#47;先内存判断  如果不成功或者内存时间失效 则读取数据库 然后判断

	var isLogin=false;
	var isAdmin=false;	
	var userinfo=null; 
	var timeout=false;
 
	&#47;&#47;查找某个用户信息  
&#47;&#47;  这样处理的话就不能再 好几个地方登陆 ,所以 处理下  还是使用 FLAG_ISLOGIN+sid+TOKEN这样的方式， 一个人可以多个地址登陆 
&#47;&#47;但是这样容易引起内存持续增大 所以要靠一个定时去持续进行内存清理，或者getCache的时候 最大的有效时间为多长，查过这个如果获取为空 则自动进行清理 在getCache中清理
	var tokenCache=FileUtils.getCache(FLAG_ISLOGIN+sid);
	if(!isEmpty(tokenCache)){
	&#47;&#47;		 isLogin=isLoginCache;
		if(tokenCache==TOKEN){
			isLogin=true; 
		}

		if(isLogin){
			var isAdminCache=FileUtils.getCache(FLAG_ISADMIN+sid); 
			if(!isEmpty(isAdminCache)){
				isAdmin=isAdminCache;
			}
		}else{
			&#47;&#47;timeout=FileUtils.isCacheTimeOut(FLAG_ISLOGIN+sid);
		}
	}else{
		timeout=true;&#47;&#47;超时
	} 
	
%&gt;</sql>
