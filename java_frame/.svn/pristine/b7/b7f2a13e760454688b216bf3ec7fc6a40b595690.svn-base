    <veloCitySql/>
    <sql>&lt;%
 
	var webtemplate=&quot;&#47;&quot;+ID; 
 
	var webtemplate_info=&quot;&#47;&quot;+ID+&quot;_INFO&quot;;  &#47;&#47;配置信息

var result=false;
var message=&quot;&quot;;

 
		&#47;&#47;判断文件是否存在 
		if(isEmpty(ID)){
			result=true;
			message=&quot;没有找到配置 !&quot;; 
		}else{
			&#47;&#47;文件保存 	
try{
         var fcontent_info=	FileUtils.getFromFileContent(webtemplate_info); 
	 var json_info=JavaScriptUtils.getJavaScript(fcontent_info);
	 
			var info=json_info[&quot;info&quot;]+&quot;&quot;;
		 var outHtml=&quot;&quot;;
		 
			var flag=json_info[&quot;flag&quot;]+&quot;&quot;;
			 outHtml=&quot;&lt;!-- &quot;+info+&quot;  --&gt;\r\n&quot;;
  
			outHtml=outHtml+DELIMITER_STATEMENT_START+&quot;  \r\n  if(isEmpty(data)){ data=\&quot;\&quot;; } \r\n include(INCLUDE_START,{data:data,FLAG:&quot;+flag+&quot;}){ }  \r\n  &quot;+DELIMITER_STATEMENT_END+&quot;\r\n&quot; ;
  
			var modal=json_info[&quot;modal&quot;];
	 
	 
        var  fcontent=	FileUtils.getFromFileContent(webtemplate); 
		var json=JavaScriptUtils.getJavaScript(fcontent);
		
		&#47;&#47;json循环 输出内容
		for(var element in json){ 
			outHtml=outHtml+element[&quot;control_html&quot;]+&quot;\r\n&quot;;	 
		}  
		
		outHtml=outHtml+DELIMITER_STATEMENT_START+&quot;  include(INCLUDE_END){ } &quot;+DELIMITER_STATEMENT_END+&quot;\r\n&quot;; &#47;&#47;END
	 
			var file=json_info[&quot;file&quot;]+&quot;&quot;; 
			FileUtils.saveFileContent(&quot;&#47;&quot;+modal+&quot;&#47;&quot;+file+&quot;.html&quot;,&quot;UTF-8&quot;,outHtml);
			
      		&#47;&#47;读取内容后 进行
			message=&quot;代码生成成功 !&quot;;
			result=true;
}catch(error){
       message=&quot;代码生成发生错误!&quot;;
       debug(message+error);
}
		}
		 	


%&gt;</sql>
    <parameter>
